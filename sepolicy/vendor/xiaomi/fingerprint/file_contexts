# Fingerprint device
/dev/goodix_fp                                                                     u:object_r:fingerprint_device:s0

# Fingerprint data
/data/vendor/fpc(/.*)?                                                             u:object_r:vendor_fingerprint_data_file:s0
/data/vendor/fpdump(/.*)?                                                          u:object_r:vendor_fingerprint_data_file:s0
/data/vendor/goodix(/.*)?                                                          u:object_r:vendor_fingerprint_data_file:s0
/mnt/vendor/persist/fpc(/.*)?                                                      u:object_r:vendor_fingerprint_data_file:s0
/mnt/vendor/persist/goodix(/.*)?                                                   u:object_r:vendor_fingerprint_data_file:s0
# udfps_pressed sensor
genfscon sysfs /devices/platform/goodix_ts.0/udfps_pressed u:object_r:vendor_sysfs_sensors:s0
# double tap sensor
genfscon sysfs /devices/platform/goodix_ts.0/double_tap_pressed u:object_r:vendor_sysfs_sensors:s0
genfscon sysfs /devices/platform/goodix_ts.0/double_tap_enabled u:object_r:vendor_sysfs_sensors:s0
